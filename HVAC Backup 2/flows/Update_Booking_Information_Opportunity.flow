            <field>EGM_Estimated_Gross_Margin__c</field>
            <value>
                <elementReference>EGM</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>EGM__c</field>
            <value>
                <elementReference>EGMPercentage</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Total_Downloading_Value__c</field>
            <value>
                <elementReference>TDV</elementReference>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Get_Downloading_Value</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Contract_Type__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Contract_Value__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>StageName</field>
            <operator>NotEqualTo</operator>
