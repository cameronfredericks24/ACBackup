    <decisions>
        <name>Is_RMR_created</name>
        <label>Is RMR created?</label>
        <locationX>611</locationX>
        <locationY>276</locationY>
        <defaultConnectorLabel>RMR Created</defaultConnectorLabel>
        <rules>
            <name>RMR_Not_Created</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Canceled</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RecordType.DeveloperName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RMR</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Return_Product_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Delivered</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Return_Product_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Received</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Transaction_Note_Record_Type_Debit_Note</targetReference>
            </connector>
            <label>RMR Not Created</label>
        </rules>
    </decisions>
