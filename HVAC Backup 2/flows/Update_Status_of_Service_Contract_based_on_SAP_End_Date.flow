    <decisions>
        <name>Check_if_Original_End_Date_is_blank</name>
        <label>Check if Original End Date is blank</label>
        <locationX>842</locationX>
        <locationY>863</locationY>
        <defaultConnector>
            <targetReference>Update_end_dates</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>if_blank</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Original_End_Date__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_original_end_date</targetReference>
            </connector>
            <label>if blank</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_Status</name>
        <label>Check Status</label>
        <locationX>842</locationX>
        <locationY>323</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>SAP_End_Date_Equals_End_Date</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.SAP_End_Date__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Record.StartDate</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_debook_status</targetReference>
            </connector>
            <label>SAP End Date Equals End Date</label>
        </rules>
        <rules>
            <name>SAP_End_Date_is_greater_than_End_Date</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.SAP_End_Date__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>$Record.EndDate</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_extended_status</targetReference>
            </connector>
            <label>SAP End Date is greater than End Date</label>
        </rules>
        <rules>
            <name>SAP_End_Date_is_between_of_Start_Date_and_End_Date</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.SAP_End_Date__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>$Record.StartDate</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.SAP_End_Date__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.EndDate</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_short_closed_status</targetReference>
            </connector>
            <label>SAP End Date is between of Start Date and End Date</label>
        </rules>
    </decisions>
    <description>added fault paths</description>
    <environments>Default</environments>
    <formulas>
        <name>terms</name>
        <dataType>Number</dataType>
        <expression>(MONTH({!$Record.EndDate}) + (12*(YEAR({!$Record.EndDate})-YEAR({!$Record.StartDate}))) - MONTH({!$Record.StartDate}))</expression>
        <scale>0</scale>
    </formulas>
    <interviewLabel>Update Status of Service Contract based on SAP End Date {!$Flow.CurrentDateTime}</interviewLabel>
