        <scale>2</scale>
    </formulas>
    <formulas>
        <name>bonusFormula</name>
        <dataType>Currency</dataType>
        <expression>IF(({!Basic}+{!DA})&lt;21000,(({!Basic}+{!DA})/100)*8.33,0)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Calculated_AdditionalcostifAnyTransportationFoodAccomodationVariable</name>
        <dataType>Number</dataType>
        <expression>{!Additional_cost_if_any_e_g_transportation_food_accomodation_etc}*{!No_of_Manpower}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Calculated_FH_NH</name>
        <dataType>Number</dataType>
        <expression>{!FH_NH}*{!No_of_Manpower}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Calculated_Gratuity</name>
        <dataType>Number</dataType>
        <expression>{!Gratuity}*{!No_of_Manpower}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Calculated_MedChecks</name>
        <dataType>Number</dataType>
        <expression>{!Med_Checkups_Diwali_Sweets}*{!No_of_Manpower}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Calculated_MobileExpenses</name>
        <dataType>Number</dataType>
        <expression>{!Mobile_Expenses}*{!No_of_Manpower}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Calculated_OT</name>
        <dataType>Number</dataType>
        <expression>{!OT_Absenteeism}*{!No_of_Manpower}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Calculated_Stationery</name>
        <dataType>Number</dataType>
        <expression>{!Stationery_Minor_Consumables}*{!No_of_Manpower}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Calculated_Tools</name>
        <dataType>Number</dataType>
        <expression>{!Tools}*{!No_of_Manpower}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>CalculatedUniformShoes</name>
        <dataType>Number</dataType>
        <expression>{!Uniform_Shoes}*{!No_of_Manpower}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>ChannelPartnerCost_15_Percent</name>
        <dataType>Number</dataType>
        <expression>{!CosttoBlueStarChannelPartner_Downloading}*0.15</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>channelPartningDownloadingValue</name>
        <dataType>Number</dataType>
        <expression>((({!yearlyAmountFormula}+{!Yearly_Outsourcing_Channel_Partner_Margin})*{!daysBetweenStartAndEndDate}/365)+

 (({!Uniform_Shoes}+
		{!Tools}+
		{!Med_Checkups_Diwali_Sweets}+
		{!Mobile_Expenses}+
		{!Stationery_Minor_Consumables}+
		{!FH_NH}+
		{!Gratuity}+
		{!OT_Absenteeism}+
		{!Additional_cost_if_any_e_g_transportation_food_accomodation_etc}
		)
		*{!No_of_Manpower}
	)
 
 )</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>CostToCustomerFrom</name>
        <dataType>Number</dataType>
        <expression>{!CosttoBlueStarChannelPartner_Downloading} + {!Blue_StarEGM}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>ctbFormula</name>
        <dataType>Currency</dataType>
        <expression>{!yearlyAmountFormula}+{!Yearly_Outsourcing_Channel_Partner_Margin}+{!UniformShoes_Variable}+{!Tools_Variable}+{!MedChecks_Variable}+{!MobileExpenses_Variable}+{!Stationery_Variable}+{!FH_NH_Variable}+{!Gratuity_Variable}+{!OT_Variable}+{!AdditionalcostifAnyTransportationFoodAccomodation_Variable}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>ctcFormula</name>
        <dataType>Currency</dataType>
        <expression>{!ctbFormula}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>daysBetweenStartAndEndDate</name>
        <dataType>Number</dataType>
        <expression>({!Get_CPQ_Quote.SBQQ__Opportunity2__r.Opportunity_End_Date__c} - {!Get_CPQ_Quote.SBQQ__Opportunity2__r.Opportunity_Start_Date__c})+1</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>esicFormula</name>
        <dataType>Currency</dataType>
        <expression>IF({!grossSalaryFormula}&lt;21000,({!grossSalaryFormula}/100)*3.25,0)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>grossSalaryFormula</name>
        <dataType>Currency</dataType>
        <expression>{!Basic}+{!DA}+{!HRA}+{!MW_Revision}+{!Other_Allowance}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>HRA_Default_5_Percent</name>
        <dataType>Number</dataType>
        <expression>{!basi_da}*0.05</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>hraFormula</name>
        <dataType>Currency</dataType>
        <expression>(5/100)*({!Basic}+{!HRA})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>leaveWageFormula</name>
        <dataType>Currency</dataType>
        <expression>IF({!grossSalaryFormula}&lt;21000,{!grossSalaryFormula}/30/12*{!Leaves_to_Consider},0)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>ListUnitPriceForManPower</name>
        <dataType>Number</dataType>
        <expression>{!CosttoCustomer} / {!No_of_Manpower}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>MedCheckUpsAndDiwaliSweets</name>
        <dataType>Number</dataType>
        <expression>{!No_of_Manpower}*800</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>medicalInsuranceFormula</name>
        <dataType>Currency</dataType>
        <expression>IF({!grossSalaryFormula}&gt;21000,450,0)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>outsourcingFormula</name>
        <dataType>Currency</dataType>
        <expression>({!yearlyAmountFormula}/100)*8</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>OutsourcingValueBasedOnAMCDuration</name>
        <dataType>Number</dataType>
        <expression>if({!outsourcingFormula} &lt;&gt; {!Yearly_Outsourcing_Channel_Partner_Margin},  {!Yearly_Outsourcing_Channel_Partner_Margin} *{!daysBetweenStartAndEndDate}/365,{!outsourcingFormula})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>pfFormula</name>
        <dataType>Currency</dataType>
        <expression>IF({!grossSalaryFormula}&gt;15000,(15000/100)*13,({!grossSalaryFormula}/100)*13)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>shoesValue</name>
        <dataType>Number</dataType>
        <expression>{!No_of_Manpower}*3000</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Stationery_MinorConsumables</name>
        <dataType>Number</dataType>
        <expression>{!No_of_Manpower}*2411</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>totalMonthlyFormula</name>
        <dataType>Currency</dataType>
        <expression>{!grossSalaryFormula}+{!esicFormula}+{!pfFormula}+{!bonusFormula}+{!leaveWageFormula}+{!medicalInsuranceFormula}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>yearlyAmountFormula</name>
        <dataType>Currency</dataType>
        <expression>{!amountFormula}*12</expression>
        <scale>2</scale>
    </formulas>
