    <formulas>
        <name>daysdifference</name>
        <dataType>Number</dataType>
        <expression>{!EndDate}-{!StartDate}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>EndDate</name>
        <dataType>Date</dataType>
        <expression>IF(ISPICKVAL({!wtyTable.firstSelectedRow.WarrantyUnitOfTime},&apos;Days&apos;) ,  {!StartDate}+ {!wtyTable.firstSelectedRow.WarrantyDuration}-1,
IF(ISPICKVAL({!wtyTable.firstSelectedRow.WarrantyUnitOfTime},&apos;Months&apos;)  ,ADDMONTHS({!StartDate},{!wtyTable.firstSelectedRow.WarrantyDuration}),
ADDMONTHS({!StartDate},({!wtyTable.firstSelectedRow.WarrantyDuration}*12))))</expression>
    </formulas>
    <formulas>
        <name>StartDate</name>
        <dataType>Date</dataType>
        <expression>IF(
    NOT(ISBLANK({!recordId.Extended_Warranty_End_Date__c})), 
    {!recordId.Extended_Warranty_End_Date__c} + 1,
    {!recordId.Standard_Warranty_End_Date__c} + 1
)</expression>
    </formulas>
