                <rightValue>
                    <stringValue>L1 Passed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_2_of_Send_Mail</targetReference>
            </connector>
            <label>L1 Passed Send L2</label>
        </rules>
        <rules>
            <name>L1_Submitted_but_Failure</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>getContactRecords.Assessment_Status__c</leftValueReference>
                <operator>EqualTo</operator>
