    <formulas>
        <name>Body</name>
        <dataType>String</dataType>
        <expression>&quot;Kindly look into this case as it has not moved and customer has followed up for &quot; +{!followupTimesVariable}+&quot; times. Please take necessary action&quot;</expression>
    </formulas>
    <formulas>
        <name>followupTimesVariable</name>
        <dataType>String</dataType>
        <expression>CASE({!LatestCount}, 3, &quot;three&quot;, 5, &quot;five&quot;,7,&quot;seven&quot;,10,&quot;ten&quot;,&apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>FollowupTimeVariable</name>
        <dataType>String</dataType>
        <expression>CASE({!LatestCount}, 3, &quot;3rd&quot;, 5, &quot;5th&quot;,7,&quot;7th&quot;,10,&quot;10th&quot;,&apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>LatestCount</name>
        <dataType>Number</dataType>
        <expression>{!FollowUpCount} + 1</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>subject</name>
        <dataType>String</dataType>
        <expression>&quot;Escalation - &quot;+{!FollowupTimeVariable}+&quot; follow up captured on the case: &quot; + {!get_case.CaseNumber}</expression>
    </formulas>
