    <fields>
        <fullName>Created_after_Reopened__c</fullName>
        <externalId>false</externalId>
        <formula>AND(
  NOT(ISBLANK(Survey_Completed_Date__c)),
  IF(
    NOT(ISBLANK(Service_Ticket__r.Latest_Work_Completed_Date__c)),
    DATEVALUE(Service_Ticket__r.Latest_Work_Completed_Date__c),
    Service_Ticket__r.Completed_Date__c
  ) &lt;= Survey_Completed_Date__c
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Created after Reopened</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
