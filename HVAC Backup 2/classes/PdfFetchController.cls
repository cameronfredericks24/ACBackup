public without sharing class PdfFetchController {
    public static final String refreshToken = UtilityClass.getOrgConstantValue('SF_Refresh_Token');
    public static final String clientId = UtilityClass.getOrgConstantValue('SF_Client_Id');
    public static final String clientSecret = UtilityClass.getOrgConstantValue('SF_Client_Secret');
    public static final String oAuth_url = UtilityClass.getOrgConstantValue('SF_OAuth_Token_Url');
    public static final String pdf_link = UtilityClass.getOrgConstantValue('SF_PDF_Link');

    @future(callout=true)

    public static void fetchAndStorePdf(String workOrderId) {
        // Generate the public link for the PDF
        String vfPageUrl = '/apex/BulkPMSWorkOrderControllerPage?workOrderId=' + workOrderId;
        String baseUrl = URL.getSalesforceBaseUrl().toExternalForm();
        System.debug('baseUrl: '+baseURL);
        String pdfLink =  pdf_link + vfPageUrl;
        String accessToken = getNewAccessToken();

                        system.debug('accessToken-- '+ accessToken);


        HttpRequest req = new HttpRequest();
        req.setEndpoint(pdfLink);
        req.setMethod('GET');
        req.setHeader('Authorization', 'Bearer ' + accessToken);
        req.setHeader('Content-Type', 'application/pdf');
        req.setTimeout(120000);

                system.debug('req-- '+ req);


        Http http = new Http();
        HttpResponse res = http.send(req);

        system.debug('res-- '+ res);

        if (res.getStatusCode() == 200) {
            Blob pdfContent = res.getBodyAsBlob();

                List<Network> networkList  = new List<Network>();
            
            Profile currentUserProfile = [SELECT Id, Name FROM Profile WHERE Id = :UserInfo.getProfileId()];
            
            // Check if the profile name contains the word "Channel"
            if (currentUserProfile.Name.containsIgnoreCase('Channel')) {
                // Your logic if the profile name contains "Channel"
                System.debug('The current user has a profile related to Channel');
                
                
                networkList = [Select Id from network where Name = 'Channel Partner Portal'];
                
                system.debug('networkList-- ' + networkList);
            }
            else if( currentUserProfile.Name.containsIgnoreCase('Technician')){
                System.debug('The current user has a profile related to technician');

                networkList = [Select Id from network where Name ='Field Service Lightning'];


            }

            
            Id networkId = (!networkList.isEmpty()) ? networkList[0].Id : null;


            //Network network = [SELECT Id,Name FROM Network where Name = 'Field Service Lightning' limit 1];
            List<WorkOrder> workOrderNum=[SELECT WorkOrderNumber from WorkOrder where ID =: workOrderId Limit 1];

            ContentVersion contentVersion = new ContentVersion();
            contentVersion.Title = 'WorkPlanChecklist_' + workOrderNum[0].WorkOrderNumber;
            contentVersion.PathOnClient = 'WorkPlanChecklist_' + workOrderNum[0].WorkOrderNumber + '.pdf';
            contentVersion.VersionData = pdfContent;
            contentVersion.IsMajorVersion = true;
            contentVersion.NetworkId = networkId;
            insert contentVersion;

            ContentVersion insertedVersion = [SELECT ContentDocumentId, Id FROM ContentVersion WHERE Id = :contentVersion.Id LIMIT 1];
            String contentVersionId = insertedVersion.Id;
            ContentDistribution contentDist = new ContentDistribution();
            contentDist.Name = 'WorkPlanChecklist_' + workOrderNum[0].WorkOrderNumber;
            contentDist.ContentVersionId = contentVersionId;
            contentDist.PreferencesAllowViewInBrowser = true;
            contentDist.PreferencesAllowOriginalDownload = true;
            insert contentDist;

            contentDist = [SELECT DistributionPublicUrl FROM ContentDistribution WHERE Id = :contentDist.Id LIMIT 1];
            String publicUrl = contentDist.DistributionPublicUrl;

            if (String.isBlank(publicUrl)) {
                publicUrl = 'Link not generated';
            }

            updateWorkOrderWithLink(workOrderId, publicUrl);

        } else if (res.getStatusCode() == 302) {
            String newLocation = res.getHeader('Location');
            if (newLocation != null) {
                req.setEndpoint(newLocation);
                res = http.send(req);

                if (res.getStatusCode() == 200) {
                    Blob pdfContent = res.getBodyAsBlob();

                    List<WorkOrder> workOrderNum=[SELECT WorkOrderNumber from WorkOrder where ID =: workOrderId Limit 1];

                    ContentVersion contentVersion = new ContentVersion();
                    contentVersion.Title = 'WorkPlanChecklist_' + workOrderNum[0].WorkOrderNumber;
                    contentVersion.PathOnClient = 'WorkPlanChecklist_' + workOrderNum[0].WorkOrderNumber + '.pdf';
                    contentVersion.VersionData = pdfContent;
                    contentVersion.IsMajorVersion = true;
                    insert contentVersion;

                    ContentVersion insertedVersion = [SELECT ContentDocumentId, Id FROM ContentVersion WHERE Id = :contentVersion.Id LIMIT 1];
                    String contentVersionId = insertedVersion.Id;

                    ContentDistribution contentDist = new ContentDistribution();
                    contentDist.Name = 'Public Link to WorkPlanChecklist_' + workOrderNum[0].WorkOrderNumber;
                    contentDist.ContentVersionId = contentVersionId;
                    contentDist.PreferencesAllowViewInBrowser = true;
                    contentDist.PreferencesAllowOriginalDownload = true;
                    insert contentDist;

                    contentDist = [SELECT DistributionPublicUrl FROM ContentDistribution WHERE Id = :contentDist.Id LIMIT 1];
                    String publicUrl = contentDist.DistributionPublicUrl;

                    if (String.isBlank(publicUrl)) {
                        publicUrl = 'Link not generated';
                    }

                    updateWorkOrderWithLink(workOrderId, publicUrl);
                } else {
                    System.debug('Failed to fetch PDF after redirection. Status: ' + res.getStatus());
                }
            }
        } else {
            System.debug('Failed to fetch PDF. Status: ' + res.getStatus());
        }
    }

    // Method to update the WorkOrder record with the public URL
    public static void updateWorkOrderWithLink(String workOrderId, String publicUrl) {
        System.debug('updateWorkOrderWithLink');
        try {
            WorkOrder wo = [SELECT Id, Bulk_Checklist_Link__c FROM WorkOrder WHERE Id = :workOrderId LIMIT 1];
            wo.Bulk_Checklist_Link__c = publicUrl;
            System.debug('Workorder id-->'+wo.id);
            update wo;
        } catch (Exception e) {
            System.debug('Failed to update WorkOrder: ' + e.getMessage());
        }
    }
    // Refresh token method
    public static String getNewAccessToken() {
        
        // Prepare the request
        HttpRequest req = new HttpRequest();
        req.setEndpoint(oAuth_url);
        req.setMethod('POST');
        req.setHeader('Content-Type', 'application/x-www-form-urlencoded');

                        system.debug('clientId--'+ clientId);
                                                system.debug('clientSecret--'+ clientSecret);
                        system.debug('refreshToken--'+ refreshToken);
                        system.debug('oAuth_url--'+ oAuth_url);



        
        String requestBody = 'grant_type=refresh_token' + 
                             '&client_id=' + EncodingUtil.urlEncode(clientId, 'UTF-8') +
                             '&client_secret=' + EncodingUtil.urlEncode(clientSecret, 'UTF-8') +
                             '&refresh_token=' + EncodingUtil.urlEncode(refreshToken, 'UTF-8');
        req.setBody(requestBody);

                system.debug('req--'+ req);

        
        Http http = new Http();
        HttpResponse res = http.send(req);

                system.debug('res--'+ res);

        
        if (res.getStatusCode() == 200) {
            Map<String, Object> responseMap = (Map<String, Object>) JSON.deserializeUntyped(res.getBody());
                            system.debug('responseMap--'+ responseMap);

            return (String) responseMap.get('access_token');
        } else {
            throw new CalloutException('Failed to refresh token: ' + res.getBody());
        }
    }

