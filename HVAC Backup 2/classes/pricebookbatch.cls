global class pricebookbatch  implements Database.Batchable<SObject>,Schedulable, Database.Stateful {
    global void execute(SchedulableContext SC) {
        Integer batchSize = Integer.valueOf(System.Label.Batch_Size != null ? System.Label.Batch_Size : '200');
        Database.executeBatch(new pricebookbatch(false, ''), batchSize);
    }
    private Map<Id,Id> quoteToContractMap = new Map<Id,Id>();
    public String query;
    global pricebookbatch(Boolean overrideQuery, String query2) {
        query = 'SELECT Id,SBQQ__Quote__c FROM SBQQ__QuoteLine__c';
        if (overrideQuery) query = query2;
        System.debug('Query '+query); 
    }
    
    global Database.QueryLocator start(Database.BatchableContext bc){
        return Database.getQueryLocator(query);
    }
    
    global void execute(Database.BatchableContext bc, List<SBQQ__QuoteLine__c> scope){
        try{ 
            Map<Id,PriceBookEntry> pbeMap = new Map<Id,PriceBookEntry>();
            list<SBQQ__QuoteLine__c> qlList = New list<SBQQ__QuoteLine__c>();
            map<Id, string> familyIdToName = New map<Id, string>();
            list<Id> productIds = New list<Id>();
            set<id> quoteid=new  set<id>();
            for(SBQQ__QuoteLine__c obj: scope){
                quoteid.add(obj.SBQQ__Quote__c);
            }
            List<SBQQ__Quote__c> qt =  [SELECT Id,SBQQ__PriceBook__c FROM SBQQ__Quote__c WHERE Id in : quoteid];
            
            list<SBQQ__QuoteLine__c> selectedQuoteLines=[SELECT Id,SBQQ__Product__c,SBQQ__Quote__c from SBQQ__QuoteLine__c where SBQQ__Quote__c in : qt];
            for (SBQQ__QuoteLine__c qlw : selectedQuoteLines){
                productIds.add(qlw.SBQQ__Product__c);
            }
            map<Id, Id> productToProductEntryId = New map<Id,Id>();
            for (PriceBookEntry pbe : [SELECT Id, Product2Id FROM PriceBookEntry WHERE Pricebook2Id =: qt[0].SBQQ__PriceBook__c AND Product2Id IN : productIds and isActive = true ]) {
                productToProductEntryId.put(pbe.Product2Id, pbe.Id);
            }
            for(Id pid : productIds){
                if(!productToProductEntryId.containsKey(pid)){
                    PriceBookEntry pbe = new PriceBookEntry();
                    pbe.Pricebook2Id = qt[0].SBQQ__PriceBook__c;
                    pbe.UnitPrice = 0;
                    pbe.Product2Id = pid;
                    pbe.IsActive = true;
                    pbeMap.put(pid,pbe);
                }
            }
            if(!pbeMap.isEmpty()){
                insert pbeMap.values();
                for(PriceBookEntry pbe : pbeMap.values()){
                    productToProductEntryId.put(pbe.Product2Id, pbe.Id);
                }
            }
            for(SBQQ__QuoteLine__c qlw :selectedQuoteLines){
                SBQQ__QuoteLine__c ql = new SBQQ__QuoteLine__c();
                ql.Id = qlw.Id;
                // ql.SBQQ__Quote__c = qlw.SBQQ__Quote__c;
                if(productToProductEntryId.containsKey(qlw.SBQQ__Product__c)){
                    ql.SBQQ__PricebookEntryId__c = productToProductEntryId.get(qlw.SBQQ__Product__c);
                    system.debug('Pricebook Entry '+productToProductEntryId.get(qlw.SBQQ__Product__c));
                }
                qlList.add(ql);
            }
            if(!qlList.isEmpty()){
                SBQQ.TriggerControl.disable();
                try{
                    
                    update qlList;
                }
                finally {
                    SBQQ.TriggerControl.enable();
                }
            }
            
        }   catch(Exception e){
            Opportunity_Error_Log__c log = new Opportunity_Error_Log__c(); 
            log.Class_Name__c = 'FetchCPQQuoteLinesBatch';
            log.Exception_Message__c = e.getMessage() ;
            log.Method_Name__c = 'Execute';
            log.Line_Number__c = e.getlineNumber();
            insert log;
            
        }
        
    }
    
    global void finish(Database.BatchableContext bc){
        system.debug('Batch processed successfully');
    }
